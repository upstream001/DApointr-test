ninja_required_version = 1.3
cxx = g++-9
nvcc = /usr/local/cuda-11.6/bin/nvcc

cflags = -Wno-unused-result -Wsign-compare -DNDEBUG -g -fwrapv -O2 -Wall -fPIC -O2 -isystem /home/tianqi/miniconda3/envs/pointr/include -fPIC -O2 -isystem /home/tianqi/miniconda3/envs/pointr/include -fPIC -I/home/tianqi/miniconda3/envs/pointr/lib/python3.10/site-packages/torch/include -I/home/tianqi/miniconda3/envs/pointr/lib/python3.10/site-packages/torch/include/torch/csrc/api/include -I/home/tianqi/miniconda3/envs/pointr/lib/python3.10/site-packages/torch/include/TH -I/home/tianqi/miniconda3/envs/pointr/lib/python3.10/site-packages/torch/include/THC -I/usr/local/cuda-11.6/include -I/home/tianqi/miniconda3/envs/pointr/include/python3.10 -c
post_cflags = -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=emd -D_GLIBCXX_USE_CXX11_ABI=0 -std=c++14
cuda_cflags = -I/home/tianqi/miniconda3/envs/pointr/lib/python3.10/site-packages/torch/include -I/home/tianqi/miniconda3/envs/pointr/lib/python3.10/site-packages/torch/include/torch/csrc/api/include -I/home/tianqi/miniconda3/envs/pointr/lib/python3.10/site-packages/torch/include/TH -I/home/tianqi/miniconda3/envs/pointr/lib/python3.10/site-packages/torch/include/THC -I/usr/local/cuda-11.6/include -I/home/tianqi/miniconda3/envs/pointr/include/python3.10 -c
cuda_post_cflags = -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr --compiler-options ''"'"'-fPIC'"'"'' -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=emd -D_GLIBCXX_USE_CXX11_ABI=0 -gencode=arch=compute_86,code=compute_86 -gencode=arch=compute_86,code=sm_86 -ccbin gcc-9 -std=c++14
cuda_dlink_post_cflags = 
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc  $cuda_cflags -c $in -o $out $cuda_post_cflags





build /home/tianqi/DAPoinTr/extensions/emd/build/temp.linux-x86_64-cpython-310/emd.o: compile /home/tianqi/DAPoinTr/extensions/emd/emd.cpp
build /home/tianqi/DAPoinTr/extensions/emd/build/temp.linux-x86_64-cpython-310/emd_cuda.o: cuda_compile /home/tianqi/DAPoinTr/extensions/emd/emd_cuda.cu







